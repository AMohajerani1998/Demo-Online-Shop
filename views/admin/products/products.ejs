        <%- include('../../shared/includes/head', {title: 'products'}) %>
        <link rel="stylesheet" href="/styles/products.css" />
        <script src="/scripts/product-management.js" defer></script>
    </head>
    <body>
            <%- include('../../shared/includes/header') %>
        <main>
            <div class="area-container">
                <section id="Products-header-container">
                    <h2 class="topic-font">Product Administration</h1>
                    <h4 class="topic-font">Manage Products</h3>
                    <h2 class="topic-font">All Products</h1>
                    <a class="normal-font add-product-button" href="/admin/products/add">Add Product</a>
                </section>
                <section>
                    <ul class="products-container">
                        <% for (const product of products){ %>
                            <li>
                                <%- include('../../shared/includes/product-box', {product:product}) %>
                            </li>
                        <% } %>
                    </ul>
                </section>
            </div>
        </main>
        <%- include('../../shared/includes/footer') %>

